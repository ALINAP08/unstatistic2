{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "unstatisticdatafactapark"
		},
		"FileServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FileServer1'"
		},
		"localsqldb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'localsqldb'"
		},
		"unstatisticdbaparklinkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'unstatisticdbaparklinkedservice'"
		},
		"unstatisticuploadapp_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'unstatisticuploadapp'"
		},
		"AzureSqlDatabase_keyvault_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "unstatisticserverapark-key-connection"
		},
		"FileServer1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Users\\Alina_Parkhomenko\\Desktop\\study\\BI__\\Task_7"
		},
		"FileServer1_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "unstatisticadminapark"
		},
		"unstatistic_key_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://unstatistickeyvault.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "unstatisticdatafactapark_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"unstatisticdbaparklinkedservice_connectionString": {
						"value": "[parameters('unstatisticdbaparklinkedservice_connectionString')]"
					},
					"unstatisticuploadapp_connectionString": {
						"value": "[parameters('unstatisticuploadapp_connectionString')]"
					},
					"FileServer1_password": {
						"value": "[parameters('FileServer1_password')]"
					},
					"localsqldb_connectionString": {
						"value": "[parameters('localsqldb_connectionString')]"
					},
					"unstatistic_key_properties_typeProperties_baseUrl": {
						"value": "[parameters('unstatistic_key_properties_typeProperties_baseUrl')]"
					},
					"AzureSqlDatabase_keyvault_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AzureSqlDatabase_keyvault_properties_typeProperties_connectionString_secretName')]"
					},
					"FileServer1_properties_typeProperties_host": {
						"value": "[parameters('FileServer1_properties_typeProperties_host')]"
					},
					"FileServer1_properties_typeProperties_userId": {
						"value": "[parameters('FileServer1_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "unstatisticdatafactapark_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'unstatisticdatafactapark_ArmTemplate_0')]"
			]
		}
	]
}