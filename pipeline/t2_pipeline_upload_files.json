						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "ds_meta_databases",
						"type": "DatasetReference",
						"parameters": {
							"Name": {
								"value": "@pipeline().parameters.Name",
								"type": "Expression"
							}
						}
