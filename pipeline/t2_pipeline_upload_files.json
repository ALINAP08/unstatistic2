				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('lkp_databases').output.value",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Execute t1_pipeline_upload_files",
							"type": "ExecutePipeline",
							"dependsOn": [],
							"userProperties": [],
							"typeProperties": {
								"pipeline": {
									"referenceName": "t1_pipeline_upload_files",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true,
								"parameters": {
									"Name": {
										"value": "@item().Name",
										"type": "Expression"
									},
									"Container": {
										"value": "@item().Container",
										"type": "Expression"
									}
								}
							}
						}
					]
				}
			}
		],
		"parameters": {
			"Name": {
				"type": "string",
				"defaultValue": "Nfield-US"
			}
		},
		"folder": {
			"name": "for KGND"
		},
		"annotations": []
	}
